class Breakfast {
  cook() {
    print "Eggs a-fryin'!";
  }

  serve(who) {
    print "Enjoy your breakfast, " + who + ".";
  }
}

class DevonshireCream {
  serveOn() {
    return "Scones";
  }
}

print DevonshireCream;

